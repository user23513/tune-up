<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="co.up.tune.aprv.aprvReq.mapper.AprvReqMapper">


	<!-- 나의조회 -->
	<select id="aprvList" parameterType="co.up.tune.aprv.vo.AprvVO" resultType="co.up.tune.aprv.vo.AprvVO">
		SELECT *
		FROM APRV
		WHERE EMP_NO = #{empNo}
		<if test = "reqSt != null">AND REQ_ST = #{reqSt}</if>
		ORDER BY APRV_NO DESC
	</select>


	






</mapper>